      <Button fx:id="primaryButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onAction="#switchToSecondary" prefWidth="120.0" text="Login" VBox.vgrow="NEVER" />
